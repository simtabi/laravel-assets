{
    "name": "simtabi/laravel-assets",
    "description": "A highly opinionated assets management library for the Laravel framework.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel cms",
        "laravel assets",
        "blade assets"
    ],
    "homepage": "https://simtabi.com",
    "authors": [
        {
            "name": "Imani Manyara",
            "role": "Developer",
            "homepage": "https://simtabi.com"
        },
        {
            "name": "Easter Mukora",
            "role": "Developer",
            "homepage": "https://simtabi.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Simtabi\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simtabi\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Simtabi\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Simtabi\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
